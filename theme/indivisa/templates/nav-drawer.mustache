{{!
    @template theme_moove/nav-drawer

    Nav drawer template

    Example context (json): {}
}}
<div id="nav-drawer-header">
    <span id="logo-universidad">
        <img src="{{{output.get_logo_universidad}}}" alt="{{sitename}}">
    </span>
</div>
<div id="nav-drawer" data-region="drawer" class="hidden-print moodle-has-zindex {{^navdraweropen}}closed{{/navdraweropen}}" aria-hidden="{{#navdraweropen}}false{{/navdraweropen}}{{^navdraweropen}}true{{/navdraweropen}}" tabindex="-1">
    {{> theme_indivisa/flat_navigation }}

    <br>
    <div class="container">
        <h4 class="text-san14">Ayuda</h4>
        <h4 class="text-san14">Soporte</h4>
    </div>
</div>
<div id="nav-drawer-footer">
    <span id="social-icons">
        <a target="_blank" href="#">
            <span id="icones"><i class="footer-icon facebook"></i></span>
        </a>
        <a target="_blank" href="#">
            <span id="icones"><i class="footer-icon twitter"></i></span>
        </a>
        <a target="_blank" href="#">
            <span id="icones"><i class="footer-icon instagram"></i></span>
        </a> 
        <a target="_blank" href="#">
            <span id="icones"><i class="footer-icon whatsapp"></i></span>
        </a> 
    </span>
</div>
